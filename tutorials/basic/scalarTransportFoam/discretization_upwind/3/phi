FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "3";
    object      phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 4;

boundaryField
{
    sides
    {
        type            calculated;
        value           nonuniform List<scalar> 2(4 -4);
    }
    empty
    {
        type            empty;
        value           nonuniform 0();
    }
}


